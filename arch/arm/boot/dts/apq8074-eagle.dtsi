&spi_epm {
	status = "disabled";
};

&pm8941_lsid1 {

    qcom,leds@d000 {
        qcom,rgb_2 {
            status = "ok";
            qcom,default-state = "on";
			#qcom,turn-off-delay-ms = <1000>;
            qcom,idx-len = <1>;
            qcom,duty-pcts = [40];
        };
		/*
        qcom,rgb_1 {
            status = "ok";
            qcom,default-state = "on";
            qcom,idx-len = <1>;
            qcom,duty-pcts = [40];
        };*/
    };

	/* channel 1 */
	pwm@b200 {
		status = "disabled";
		compatible = "qcom,qpnp-pwm";
		reg = <0xb200 0x100>,
		      <0xb042 0x7e>;
		reg-names = "qpnp-lpg-channel-base", "qpnp-lpg-lut-base";
		qcom,channel-id = <1>;

        /* override */
		qcom,mode-select = <0>; /* pwm */
		qcom,period = <10>; /* 10 microseconds */
		qcom,force-pwm-size = <9>;
		qcom,pwm {
			qcom,duty = <5>; /* 5 microseconds */
			label = "pwm";
		};
	};
};

